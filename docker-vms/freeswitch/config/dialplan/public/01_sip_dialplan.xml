<include>
    <extension name="sip.inbound">
        <condition field="destination_number" expression=".*">
            <action application="log" data="INFO DIALING Extension sip.inbound From [${sip_from_user}] To [${sip_to_user}]"/>
            <action application="socket" data="127.0.0.1:8050 async full"/>
        </condition>
    </extension>
</include>
